&FORCE_EVAL
  METHOD Quickstep
  &DFT
    LSD
    CHARGE -1
    BASIS_SET_FILE_NAME ../PBE0_UCSEGBASIS
    POTENTIAL_FILE_NAME ../POTENTIAL
    &MGRID
      CUTOFF 1600
      REL_CUTOFF  80
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-14
    &END QS
    &SCF
      EPS_SCF 1.0E-6
      MAX_SCF 40
      SCF_GUESS ATOMIC
      &OT
          PRECONDITIONER FULL_ALL
          MINIMIZER CG
          LINESEARCH 2PNT
      &END
      &OUTER_SCF
         EPS_SCF 1.0E-6
         MAX_SCF 5
      &END
    &END SCF
#    &XC
#      &XC_FUNCTIONAL PBE
#      &END XC_FUNCTIONAL
#    &END XC
     &XC
      &XC_FUNCTIONAL
        &PBE
          SCALE_X 0.75
          SCALE_C 1.0
        &END
      &END XC_FUNCTIONAL
      &HF
        &SCREENING
          EPS_SCHWARZ 1.0E-12
          SCREEN_ON_INITIAL_P F
        &END
        &MEMORY
          MAX_MEMORY 6000
          EPS_STORAGE_SCALING 0.1
        &END
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE COULOMB
        &END
        FRACTION 0.25
      &END
    &END XC
    &POISSON
        PERIODIC NONE
        POISSON_SOLVER MT
    &END
