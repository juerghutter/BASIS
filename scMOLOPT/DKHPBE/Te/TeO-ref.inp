&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME ../def2QZVPP
    POTENTIAL_FILE_NAME ../POTENTIAL
    &MGRID
      CUTOFF 600
      REL_CUTOFF  60
    &END MGRID
    &QS
      METHOD GAPW
      EPS_DEFAULT 1.0E-14
    &END QS
    &SCF
      EPS_SCF 3.0E-7
      MAX_SCF 90
      #SCF_GUESS ATOMIC
      SCF_GUESS RESTART
      &OT
         #MINIMIZER DIIS
         MINIMIZER CG
         LINESEARCH 3PNT
         PRECONDITIONER FULL_ALL
         STEPSIZE 0.1
      &END OT
      &OUTER_SCF
        EPS_SCF 3.0E-7
        MAX_SCF 1
      &END 
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 12.0 12.0 12.0
    &END CELL
    &COORD
Te       8.00000000       8.00000000       7.05861395   
O       8.00000000       8.00000000       8.94138605   
    &END COORD
    &KIND Te
      BASIS_SET def2-QZVPP
      POTENTIAL GTH-PBE-q24
    &END KIND
    &KIND O
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT refTeOq6
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL
