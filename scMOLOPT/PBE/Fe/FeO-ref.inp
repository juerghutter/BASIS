&FORCE_EVAL
  METHOD Quickstep
  &DFT
    LSD
    MULTIPLICITY 5
    BASIS_SET_FILE_NAME ../def2QZVPP
    POTENTIAL_FILE_NAME ../POTENTIAL
    &MGRID
      CUTOFF 600
      REL_CUTOFF  60
    &END MGRID
    &QS
      METHOD GAPW
      EPS_DEFAULT 1.0E-14
    &END QS
    &SCF
      EPS_SCF 5.0E-7
      MAX_SCF 20
      #SCF_GUESS ATOMIC
      SCF_GUESS RESTART
      &OT
         MINIMIZER DIIS
         #MINIMIZER CG
         LINESEARCH 3PNT
         PRECONDITIONER FULL_ALL
         STEPSIZE 0.1
      &END OT
      &OUTER_SCF
        EPS_SCF 5.0E-7
        MAX_SCF 1
      &END
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 15.0 15.0 15.0
    &END CELL
    &COORD
Fe       6.70500742       7.50000000       7.50000000       26
O       8.29499258       7.50000000       7.50000000        8
    &END COORD
    &KIND Fe
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
      MAGNETIZATION 2
      LEBEDEV_GRID 100
      RADIAL_GRID  250
    &END KIND
    &KIND H
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
    &KIND C
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
    &KIND O
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
    &KIND F
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT refFeO
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
  PREFERRED_DIAG_LIBRARY SL
&END GLOBAL
