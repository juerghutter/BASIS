&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME ../def2QZVPP
    POTENTIAL_FILE_NAME ../POTENTIAL
    &MGRID
      CUTOFF 600
      REL_CUTOFF  60
    &END MGRID
    &QS
      METHOD GAPW
      EPS_DEFAULT 1.0E-14
    &END QS
    &SCF
      EPS_SCF 1.0E-7
      MAX_SCF 20
      SCF_GUESS ATOMIC
      &OT
         MINIMIZER DIIS
         PRECONDITIONER FULL_ALL
         STEPSIZE 0.1
      &END OT
      &OUTER_SCF
        EPS_SCF 1.0E-7
        MAX_SCF 5
      &END 
    &END SCF
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 12.0 12.0 12.0
    &END CELL
    &COORD
Ca       9.00000000       9.00000000       9.00000000       
F       9.00000000       9.00000000       6.89840952       
F       9.00000000       9.00000000      11.10159048      
    &END COORD
    &KIND Ca
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
    &KIND H
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
    &KIND F
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
    &KIND Cl
      BASIS_SET def2-QZVPP
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT refCaF2
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
  PREFERRED_DIAG_LIBRARY SL
&END GLOBAL
